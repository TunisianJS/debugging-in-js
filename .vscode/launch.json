{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:3000"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Docker container",
            "localRoot": "${workspaceRoot}/server/src",
            "remoteRoot": "/app/src",
            "port": 9229
        },
    ]
}